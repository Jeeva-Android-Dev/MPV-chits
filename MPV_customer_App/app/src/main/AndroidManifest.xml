<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.mazenet.mzs119.mpvmemberapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name="com.mazenet.mzs119.mpvmemberapp.Utils.AppController"
        android:allowBackup="true"
        android:icon="@drawable/mpvlogo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/mpvlogo"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/AppTheme"
        android:appComponentFactory="@string/app_name"
        tools:replace="android:appComponentFactory">
        <uses-library
            android:name="org.apache.http.legacy"
            android:required="false" />
        <activity
            android:name="com.mazenet.mzs119.mpvmemberapp.MainActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity
            android:name="com.mazenet.mzs119.mpvmemberapp.SplashScreen"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.mazenet.mzs119.mpvmemberapp.ChangePassword" />
        <activity
            android:name="com.mazenet.mzs119.mpvmemberapp.MenuActivity"
            android:screenOrientation="portrait" />
        <activity android:name="com.mazenet.mzs119.mpvmemberapp.MyProfile" />
        <activity android:name="com.mazenet.mzs119.mpvmemberapp.MyChits" />
        <activity android:name="com.mazenet.mzs119.mpvmemberapp.MYAccountCopy" />
        <activity android:name="com.mazenet.mzs119.mpvmemberapp.MyOutstandings" />
        <activity android:name="com.mazenet.mzs119.mpvmemberapp.NewCommencedChits"></activity>
        <activity android:name="com.mazenet.mzs119.mpvmemberapp.MyTransactions" />



    </application>

</manifest>